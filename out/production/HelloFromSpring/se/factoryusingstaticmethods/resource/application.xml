<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="org.springframework.beans.factory.config.MethodInvokingFactoryBean">
        <property name="staticMethod" value="se.factoryusingstaticmethods.CarFactory.setCarname"/>
        <property name="arguments">
            <list>
                <value>se.factoryusingstaticmethods.Honda</value>
            </list>
        </property>
    </bean>
    <bean id="cf" class="se.factoryusingstaticmethods.CarFactory" factory-method="getInstance">

    </bean>


</beans>